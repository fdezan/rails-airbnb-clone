<div class="proposal-area col-xs-12 col-sm-4">
   <!--  <h5> Criar Proposta</h5> -->
    <%= simple_form_for [@service, @proposal] do |f| %>
      <%= f.input :description, label: 'Criar Proposta', input_html: { style: 'width: 800px;
    height: 170px;'}  %>
      <%= f.input :user_id, :as => :hidden, label: 'User ID' %>
      <%= f.input :service_id, :as => :hidden,label: 'Service ID' %>
      <% f.input :photos, as: :attachinary %>
      <%= f.submit "Adicionar", class: "btn btn-primary" %>
      <%= link_to "Voltar", services_path(category: @service.category), :method => "get", :class=>"btn btn-warning" %>
    <% end %>
</div>

